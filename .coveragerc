[run]
source = .
omit =
    */venv/*
    */build/*
    */dist/*
    */__pycache__/*
    */.hypothesis/*
    */tests/*
    */test_*.py
    setup.py
    */scripts/*

[report]
exclude_lines =
    pragma: no cover
    def __repr__
    raise AssertionError
    raise NotImplementedError
    if __name__ == .__main__.:
    if TYPE_CHECKING:
    @abstractmethod

[html]
directory = htmlcov
